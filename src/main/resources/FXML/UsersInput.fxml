<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.UsersInputController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="10.0" prefHeight="248.0" prefWidth="1200.0">
         <children>
            <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#returnToUser" prefHeight="41.0" prefWidth="55.0" text="Return">
                     <HBox.margin>
                        <Insets right="15.0" top="-5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Text fx:id="productionTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Create user" textAlignment="CENTER" underline="true">
               <font>
                  <Font size="36.0" />
               </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Text>
            <TextField fx:id="username" promptText="Username">
               <font>
                  <Font size="24.0" />
               </font>
            </TextField>
            <TextField fx:id="password" promptText="Password">
               <font>
                  <Font size="24.0" />
               </font>
            </TextField>
         </children>
      </VBox>
      <Button layoutX="21.0" layoutY="301.0" mnemonicParsing="false" text="Add user" />
      <CheckBox fx:id="isAdminCheckbox" layoutX="27.0" layoutY="274.0" mnemonicParsing="false" text="Admin" />
   </children>
</AnchorPane>
